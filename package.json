{
  "name": "@ohbug/unplugin",
  "version": "0.0.0",
  "description": "Unplugin for upload your sourceMap files to Ohbug",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "ohbug"
  ],
  "author": "chenyueban <jasonchan0527@gmail.com>",
  "homepage": "https://github.com/ohbug-org/unplugin-ohbug",
  "bugs": {
    "url": "https://github.com/ohbug-org/unplugin-ohbug/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ohbug-org/unplugin-ohbug"
  },
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "play": "npm -C playground run dev",
    "release": "release-it",
    "start": "esno src/index.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ohbug/cli": "^1.1.0",
    "@ohbug/types": "^1.1.4",
    "unplugin": "^0.2.9"
  },
  "devDependencies": {
    "@chenyueban/lint": "^1.1.0",
    "@chenyueban/tsconfig": "^0.2.0",
    "@release-it/conventional-changelog": "^3.3.0",
    "@types/express": "^4.17.11",
    "@types/multer": "^1.4.5",
    "@types/rimraf": "^3.0.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "esno": "^0.9.1",
    "express": "^4.17.1",
    "fast-glob": "^3.2.7",
    "multer": "^1.4.2",
    "nodemon": "^2.0.12",
    "release-it": "^14.11.5",
    "rimraf": "^3.0.2",
    "tsup": "^4.14.0",
    "typescript": "^4.4.3",
    "vite": "^2.5.3",
    "webpack": "^5.52.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
